{
    "name": "osushi/elasticsearch-orm",
    "description": "O/R Mapper For ElasticSearch Like a Laravel Eloquent",
    "keywords": ["elasticsearch", "es", "orm", "model", "laravel", "eloquent"],
    "license": "MIT",
    "type": "package",
    "homepage": "https://github.com/Osushi/ElasticsearchOrm",
    "authors": [
        {
            "name": "Osushi",
            "homepage": "https://github.com/Osushi",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "elasticsearch/elasticsearch": "~5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Osushi\\ElasticsearchOrm\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osushi\\ElasticsearchOrm\\Providers\\ElasticsearchOrmServiceProvider"
            ]
        }
    }
}
